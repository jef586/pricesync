{
  "name": "prycesync-mobile",
  "version": "1.0.0",
  "description": "Mobile app for PryceSync ERP sales team",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5174",
    "build": "vite build --config vite.config.compatible.ts",
    "build:node18": "vite build --config vite.config.node18.ts",
    "preview": "vite preview --host 0.0.0.0 --port 4174",
    "type-check": "vue-tsc --noEmit",
    "capacitor:sync": "capacitor sync",
    "capacitor:copy": "capacitor copy",
    "capacitor:open:android": "capacitor open android",
    "capacitor:open:ios": "capacitor open ios",
    "capacitor:run:android": "capacitor run android",
    "capacitor:run:ios": "capacitor run ios",
    "android:build": "npm run build && capacitor sync android && cd android && ./gradlew assembleRelease",
    "android:build:dev": "npm run build && capacitor sync android && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@ionic/vue": "^8.0.0",
    "@ionic/vue-router": "^8.0.0",
    "axios": "^1.12.2",
    "ionicons": "^8.0.13",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
